@media (min-width: 320px) and (max-width: 520px) {
  .carrusel {
    width: 95%;
    margin: auto;
    overflow: hidden;
  }
  .carrusel ul {
    text-align: center;
    padding: 0;
    display: flex;
    width: 200%;
    animation: cambio 15s infinite;
    animation-timing-function: ease-out;
    justify-content: center;
    align-items: center;
  }
  .carrusel ul li {
    width: 100%;
    list-style: none;
  }
  .carrusel ul li img {
    width: 100%;
  }
  .sedeCentral, .sedeRayo, .sedeQuilmes, .sedeRioja, .sedeRosario, .sedeDragones {
    color: #e2e2e2;
    border-radius: 2rem;
    background: linear-gradient(-45deg, rgba(19, 26, 53, 0.6784313725) 50%, #001f3a 50%);
    margin: 3rem;
    height: auto;
    border: 0.1rem solid white;
    display: flex;
    justify-content: center;
    text-align: center;
    flex-direction: column;
  }
  .parteUno h3 {
    font-size: 2rem;
    margin: 1rem;
    font-weight: 600;
    text-transform: uppercase;
    text-decoration: underline;
  }
  .parteUno h4 {
    margin-bottom: 1.5rem;
    font: 1.8rem "Roboto Condensed", sans-serif;
    font-weight: 500;
  }
  .parteUno h4:nth-child(3) {
    margin-bottom: 3rem;
  }
  .parteUno h4:nth-child(4) {
    margin-bottom: 2rem;
  }
  .parteUno ul li {
    list-style: none;
    margin-bottom: 1rem;
    font: 1.5rem "Roboto Condensed", sans-serif;
    font-weight: 400;
  }
  .parteDos {
    margin-top: 3rem;
  }
  .parteDos .cardEnlaces {
    display: flex;
    justify-content: space-around;
    align-items: flex-end;
  }
  .parteDos nav .cardEnlaces li {
    list-style: none;
  }
  .parteDos nav ul li a img {
    width: 3rem;
    transition: all 350ms ease-in;
  }
  .parteDos nav ul li:first-child a img {
    width: 3rem;
    transition: all 350ms ease-in;
  }
  .parteDos nav ul li a:nth-child(1) img:hover {
    width: 4rem;
    background-color: rgba(0, 0, 0, 0.457);
    border-radius: 100%;
    padding: 0.6rem;
  }
}
@keyframes cambio {
  0% {
    margin-left: 0;
  }
  30% {
    margin-left: 0;
  }
  50% {
    margin-left: -100%;
  }
  100% {
    margin-left: -100%;
  }
}
@media (min-width: 320px) and (max-width: 520px) {
  .contenedorTituloParrafo {
    display: flex;
    text-align: center;
    justify-content: center;
    flex-direction: column;
  }
  .contenedorTituloParrafo h1, .contenedorTituloParrafo h2 {
    font: 3rem "Roboto Condensed", sans-serif;
    margin: 7rem 1rem 0rem 1rem;
    color: rgb(255, 255, 255);
  }
  .contenedorTituloParrafo p {
    margin: 3rem 2rem;
    font: 1.5rem "Roboto Condensed", sans-serif;
    font-weight: 800;
    text-transform: uppercase;
    text-align: center;
    color: rgb(255, 255, 255);
    line-height: 3rem;
  }
  header figure {
    text-align: center;
  }
  header figure img {
    width: 30rem;
  }
  .contenedorIndex {
    display: grid;
    grid-template-areas: "sede " "instuctor" "eventos" "contacto";
    margin: 2rem;
  }
  .contenedorIndex .cardSede {
    margin-top: 7rem;
    grid-area: sede;
    display: grid;
    grid-template-areas: "techo" "titulo" "parrafo" "piso ";
    background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.2)), url(/imagenes/fondoCards/guantes.jpg);
    background-position: top left;
    background-size: cover;
  }
  .contenedorIndex .cardSede:hover {
    border-radius: 0.25rem;
  }
  .contenedorIndex .cardInstructor {
    margin-top: 4rem;
    grid-area: instuctor;
    display: grid;
    grid-template-areas: "techo" "titulo" "parrafo" "piso ";
    background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.2)), url(/imagenes/fondoCards/guantesDos.jpg);
    background-position: bottom left;
    background-size: cover;
  }
  .contenedorIndex .cardEventos {
    margin-top: 4rem;
    grid-area: eventos;
    display: grid;
    grid-template-areas: "techo" "titulo" "parrafo" "piso ";
    background-image: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.2)), url(/imagenes/fondoCards/sede.jpg);
    background-position: right;
    background-size: cover;
  }
  .contenedorIndex .cardContactos {
    margin-top: 4rem;
    grid-area: contacto;
    display: grid;
    grid-template-areas: "techo" "titulo" "parrafo" "piso ";
    background-image: linear-gradient(rgba(0, 0, 0, 0.8), rgba(0, 0, 0, 0.2)), url(/imagenes/fire-and-water-2354583_640.jpg);
    background-position: right;
    background-size: cover;
  }
  .cardEventos .tituloCard {
    font: 3rem "Roboto Condensed", sans-serif;
    margin-top: 2rem;
    margin-left: 1rem;
    color: rgb(255, 255, 255);
    grid-area: titulo;
  }
  .techoCard {
    background-color: rgba(77, 76, 76, 0.71);
    height: 2.2rem;
    grid-area: techo;
    border-radius: 0.25rem;
  }
  .tituloCard {
    font: 3rem "Roboto Condensed", sans-serif;
    margin-top: 2rem;
    margin-left: 1rem;
    color: rgb(255, 255, 255);
    grid-area: titulo;
  }
  .parrafoCard {
    margin: 1rem 0.6rem;
    font: 1.2rem "Roboto Condensed", sans-serif;
    font-weight: 800;
    text-transform: uppercase;
    text-align: left;
    color: rgb(255, 255, 255);
    line-height: 3rem;
    grid-area: parrafo;
  }
  .pisoCard {
    background-color: rgba(77, 76, 76, 0.71);
    height: 2.2rem;
    grid-area: piso;
    border-radius: 0.25rem;
  }
}
