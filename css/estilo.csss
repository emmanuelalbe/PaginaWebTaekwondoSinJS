@media (min-width: 320px) and (max-width: 520px) {
  * {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
  }
  html {
    font-size: 62.5%;
  }
  .color {
    background: rgba(77, 76, 76, 0.71);
    font: 2rem "Roboto Condensed", sans-serif;
    text-transform: uppercase;
    font-weight: 800;
    text-align: center;
  }
  .nav-item {
    margin-top: 5rem;
    margin-bottom: 2.5rem;
    transition: all 700ms;
  }
  .nav-link {
    color: rgb(255, 255, 255);
    transition: all 320ms;
  }
  .nav-link:hover {
    color: rgba(118, 126, 237, 0.589);
  }
  .nav-link:active {
    color: rgba(22, 87, 96, 0.589);
  }
  /*Estilo Footer*/
  body {
    background-image: linear-gradient(rgb(0, 0, 0), rgba(0, 0, 0, 0.7)), url(../imagenes/descarga\ \(3\).jpg);
    background-attachment: fixed;
    background-position: top center;
    background-size: cover;
  }
  body footer {
    margin-top: 4rem;
  }
  body footer nav ul {
    display: flex;
    justify-content: flex-end;
    align-items: center;
  }
  body footer nav ul li {
    margin-right: 2rem;
    list-style: none;
  }
  body footer nav ul li a img {
    width: 4rem;
  }
}
